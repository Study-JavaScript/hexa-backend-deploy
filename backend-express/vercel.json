{
    "version": 2,
    "builds": [
      {
        "src": "index.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "../infrastructure/**",
            "../domain/**",
            "../application/**",
            "../utils/**"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.ts"
      }
    ]
  }